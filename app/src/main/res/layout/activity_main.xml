<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.prathabodas.cupertinoconnect.MainActivity">

    <Button
        android:id="@+id/viewLoc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="View Location"
        android:layout_marginBottom="11dp"
        android:layout_above="@+id/viewReq"
        android:layout_alignLeft="@+id/viewReq"
        android:layout_alignStart="@+id/viewReq" />

    <Button
        android:id="@+id/viewReq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="138dp"
        android:text="View Requests" />

</RelativeLayout>
